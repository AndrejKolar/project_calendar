<?xml version="1.0"?>
<ss:Workbook xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet">
    <ss:Worksheet ss:Name="Sheet1">
        <ss:Table>
            <ss:Row>
                <ss:Cell>
                    <ss:Data ss:Type="String">Title</ss:Data>
                </ss:Cell>
                <ss:Cell>
                    <ss:Data ss:Type="String">Date</ss:Data>
                </ss:Cell>
                <ss:Cell>
                    <ss:Data ss:Type="String">Hours</ss:Data>
                </ss:Cell>
                <ss:Cell>
                    <ss:Data ss:Type="String">Project</ss:Data>
                </ss:Cell>
                <ss:Cell>
                    <ss:Data ss:Type="String">Starting time</ss:Data>
                </ss:Cell>
                <ss:Cell>
                    <ss:Data ss:Type="String">Ending time</ss:Data>
                </ss:Cell>
                <ss:Cell>
                    <ss:Data ss:Type="String">Description</ss:Data>
                </ss:Cell>
            </ss:Row>
             <% @events.each do |event| %>
            <ss:Row>
                <ss:Cell>
                    <ss:Data ss:Type="String"><%= event.title %></ss:Data>
                </ss:Cell>
                <ss:Cell>
                    <ss:Data ss:Type="Date"><%= event.starts_at.to_date %></ss:Data>
                </ss:Cell>
                <ss:Cell>
                    <ss:Data ss:Type="String"><%= event.total_hours %></ss:Data>
                </ss:Cell>
                <ss:Cell>
                    <ss:Data ss:Type="Date"><%= event.starts_at.strftime("%H:%M") %></ss:Data>
                </ss:Cell>
                <ss:Cell>
                    <ss:Data ss:Type="Date"><%= event.ends_at.strftime("%H:%M") %></ss:Data>
                </ss:Cell>
                <ss:Cell>
                    <ss:Data ss:Type="String"><%= event.description %></ss:Data>
                </ss:Cell>
            </ss:Row>
            <% end %>
        </ss:Table>
    </ss:Worksheet>
</ss:Workbook>